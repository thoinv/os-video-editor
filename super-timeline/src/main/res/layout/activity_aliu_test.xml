<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#003234"
    android:orientation="vertical">

    <Button
        android:id="@+id/bt"
        android:layout_width="100dp"
        android:layout_height="50dp"
        android:text="clip" />

    <Button
        android:id="@+id/bt2"
        android:layout_width="100dp"
        android:layout_height="50dp"
        android:layout_toRightOf="@+id/bt"
        android:text="sticker" />

    <Button
        android:id="@+id/bt3"
        android:layout_width="100dp"
        android:layout_height="50dp"
        android:layout_toRightOf="@+id/bt2"
        android:text="music" />

    <com.quvideo.xiaoying.supertimeline.view.SuperTimeLineGroup
        android:id="@+id/stlg"
        android:layout_width="match_parent"
        android:layout_height="286dp"
        android:layout_alignParentBottom="true" />

    <com.quvideo.xiaoying.supertimeline.view.KitTimeLine
        android:layout_width="match_parent"
        android:layout_height="68dp"
        android:layout_marginTop="100dp"
        android:background="#123587" />
</RelativeLayout>
